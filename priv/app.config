[

%%
%% Example of application configuration
{permit, [
   
   %%
   %% RFC 7519  
   %%   4.1.1.  "iss" (Issuer) Claim
   {issuer, {env, "PERMIT_ISSUER", "permit"}}

   %%
   %% public/secret key to sign/verify the token
   %% supported schema(s)
   %%   * data:
   %%   * file:
   %%   * http(s):
   %%   * s3:
  ,{public, {env, "PERMIT_PUBLIC", "data:"}}
  ,{secret, {env, "PERMIT_SECRET", "data:"}}

   %%
   %% default list of roles 
  ,{roles,  {env, "PERMIT_ROLES", "uid"}}
]}

].